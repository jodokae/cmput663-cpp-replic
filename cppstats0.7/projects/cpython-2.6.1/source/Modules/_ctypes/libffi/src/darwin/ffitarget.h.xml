<unit xmlns="http://www.sdml.info/srcML/src" xmlns:cpp="http://www.sdml.info/srcML/cpp" language="C" filename="projects/cpython-2.6.1/source/Modules/_ctypes/libffi/src/darwin/ffitarget.h"><comment type="block">/*
 * This file is for MacOSX only. Dispatch to the right architecture include
 * file based on the current archictecture (instead of relying on a symlink
 * created by configure). This makes is possible to build a univeral binary
 * of ctypes in one go.
 */</comment>
<cpp:if>#<cpp:directive>if</cpp:directive> <expr><call><name>defined</name><argument_list>(<argument><expr><name>__i386__</name></expr></argument>)</argument_list></call></expr></cpp:if>

<cpp:ifndef>#<cpp:directive>ifndef</cpp:directive> <name>X86_DARWIN</name></cpp:ifndef>
<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>X86_DARWIN</name></cpp:macro></cpp:define>
<cpp:endif>#<cpp:directive>endif</cpp:directive></cpp:endif>
<cpp:undef>#<cpp:directive>undef</cpp:directive> <name>POWERPC_DARWIN</name></cpp:undef>

<cpp:include>#<cpp:directive>include</cpp:directive> <cpp:file>"../src/x86/ffitarget.h"</cpp:file></cpp:include>

<cpp:elif>#<cpp:directive>elif</cpp:directive> <expr><call><name>defined</name><argument_list>(<argument><expr><name>__ppc__</name></expr></argument>)</argument_list></call></expr></cpp:elif>

<cpp:ifndef>#<cpp:directive>ifndef</cpp:directive> <name>POWERPC_DARWIN</name></cpp:ifndef>
<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>POWERPC_DARWIN</name></cpp:macro></cpp:define>
<cpp:endif>#<cpp:directive>endif</cpp:directive></cpp:endif>
<cpp:undef>#<cpp:directive>undef</cpp:directive> <name>X86_DARWIN</name></cpp:undef>

<cpp:include>#<cpp:directive>include</cpp:directive> <cpp:file>"../src/powerpc/ffitarget.h"</cpp:file></cpp:include>

<cpp:endif>#<cpp:directive>endif</cpp:directive></cpp:endif>
</unit>
